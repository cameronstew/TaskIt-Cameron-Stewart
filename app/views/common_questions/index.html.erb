<div class="page-header">
  <h1 >Frequently Asked Questions</h1>
</div>

<% @CommonQuestions.each do |common_question|  %>
  <li>
    <%= link_to common_question.question, "##{common_question.slug}" %>
  </li>
<% end %>

<% @CommonQuestions.each do |common_question|  %>
  <h3 id="<%= common_question.slug %>"><strong><%=  common_question.question %></strong></h3>
  <p><%=  common_question.answer %> </p>
  <%= link_to "top", "#top" %>
<% end %>
